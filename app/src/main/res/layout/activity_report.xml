<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/background"
    android:layout_height="match_parent"
    tools:context="com.example.abdulsamadkhan.audiotest.report">

    <TextView
        android:id="@+id/tvname"
        android:layout_width="120dp"
        android:layout_height="41dp"
        android:layout_alignParentStart="true"
        android:text="Name"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="90dp"
        android:layout_above="@+id/tvresult" />


    <TextView
        android:id="@+id/tname"
        android:layout_width="294dp"
        android:layout_height="41dp"
        android:text="TextView"
        android:textSize="24sp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="90dp"
        android:layout_alignBaseline="@+id/tvname"
        android:layout_alignBottom="@+id/tvname"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/tvresult"
        android:layout_width="107dp"
        android:layout_height="41dp"
        android:layout_above="@+id/tvBp"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/tresult"
        android:text="Result"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="130dp" />

    <TextView
        android:id="@+id/tBPs"
        android:layout_width="128dp"
        android:layout_height="41dp"
        android:text="TextView"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="175dp"
        android:layout_above="@+id/tage"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/tpulse"
        android:layout_width="294dp"
        android:layout_height="41dp"
        android:text="TextView"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="257dp"
        android:layout_alignBaseline="@+id/tvpulse"
        android:layout_alignBottom="@+id/tvpulse"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/tgender"
        android:layout_width="294dp"
        android:layout_height="41dp"
        android:text="TextView"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="298dp"
        android:layout_below="@+id/tpulse"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/tvpulse" />

    <TextView
        android:id="@+id/tage"
        android:layout_width="294dp"
        android:layout_height="41dp"
        android:text="TextView"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="216dp"
        android:layout_alignBaseline="@+id/tvAge"
        android:layout_alignBottom="@+id/tvAge"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/tresult"
        android:layout_width="294dp"
        android:layout_height="41dp"
        android:text="TextView"
        android:textSize="24sp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="136dp"
        android:layout_below="@+id/tname"
        android:layout_toEndOf="@+id/tvname" />

    <TextView
        android:id="@+id/tvBp"
        android:layout_width="106dp"
        android:layout_height="41dp"
        android:layout_above="@+id/tvAge"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/tresult"
        android:text="Blood Pressure"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="177dp" />

    <TextView
        android:id="@+id/tvAge"
        android:layout_width="106dp"
        android:layout_height="41dp"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_toStartOf="@+id/tpulse"
        android:text="Age"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="216dp" />

    <TextView
        android:id="@+id/tvpulse"
        android:layout_width="106dp"
        android:layout_height="41dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tage"
        android:layout_toStartOf="@+id/tpulse"
        android:text="Pulse"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="257dp" />

    <TextView
        android:id="@+id/tvgender"
        android:layout_width="106dp"
        android:layout_height="41dp"
        android:layout_alignBaseline="@+id/tgender"
        android:layout_alignBottom="@+id/tgender"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/tgender"
        android:text="Gender"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="298dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="395dp"
        android:layout_height="74dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="9dp"
        android:text="Patient Report"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textSize="36sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp" />

    <TextView
        android:id="@+id/tBPd"
        android:layout_width="168dp"
        android:layout_height="41dp"
        android:text="TextView"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="238dp"
        tools:layout_editor_absoluteY="175dp"
        android:layout_above="@+id/tage"
        android:layout_toEndOf="@+id/tvresult" />
</RelativeLayout>